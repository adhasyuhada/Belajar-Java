package abstract;

/**
 *
 * @author anonim
 */
public class Test extends Test1{ //inheritance kelas Test1 karena menggunakan abstract maka harus di turunkan kelasnya
    
    public static void main(String[] args){
        Test1 obj1 = new Test();
        obj1.Jalan();
        obj1.Speak();
        obj1.Jump();
        
    }
    
    @Override
    void Jalan(){ //method abstract yang ada di kelas Test1
        System.out.println("prok prok");
    }

    @Override
    void Speak() {
        System.out.println("halo guys!!!");
    }

    @Override
    void Jump() {
        System.out.println("brug brug");
    }

}
